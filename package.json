{
  "name": "ai-agent-pair-trader",
  "version": "0.1.0",
  "description": "agent analyse trade pairs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "jest",
    "format": "biome format . --write",
    "lint": "lint-staged --allow-empty",
    "lint:fix": "pnpm lint --apply",
    "postinstall": "prisma generate",
    "fetch:pairs": "bun run ./public/scripts/getCoinglassPairs.ts",
    "fetch:coins": "bun run ./public/scripts/getCoins.ts",
    "finalEnums": "bun run ./public/scripts/mapPairsAndCoins.ts",
    "insert:coins": "tsx ./public/scripts/upsertCoinsToDb.ts",
    "insert:pairs": "tsx ./public/scripts/upsertPairsToDb.ts",
    "test:agent": "tsx ./src/app/api/discord/index.ts",
    "run-trader": "node --loader ts-node/esm ./src/index.ts"
  },
  "author": "yadheedhya",
  "license": "MIT",
  "dependencies": {
    "@ai16z/client-direct": "0.1.6-alpha.4",
    "@ai16z/eliza": "0.1.6-alpha.4",
    "@ai16z/plugin-0g": "0.1.6-alpha.4",
    "@prisma/client": "^6.2.1",
    "@types/react": "^19.0.4",
    "@types/react-dom": "^19.0.2",
    "axios": "^1.7.9",
    "cli-progress": "^3.12.0",
    "colors": "^1.4.0",
    "dotenv": "^16.4.7",
    "next": "^15.1.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@babel/parser": "^7.26.3",
    "@biomejs/biome": "^1.9.4",
    "@types/cli-progress": "^3.11.6",
    "@types/colors": "^1.2.4",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.17.12",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "acorn": "^8.14.0",
    "acorn-walk": "^8.3.4",
    "eslint": "^8.57.1",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "prisma": "^6.2.1",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  },
  "engines": {
    "node": ">=20.0.0",
    "pnpm": ">=9"
  },
  "lint-staged": {
    "*.{js,mjs,jsx,ts,tsx,json}": [
      "biome check --write --no-errors-on-unmatched"
    ]
  },
  "packageManager": "pnpm@9.1.3"
}